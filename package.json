{
  "name": "react-redux-parse-server",
  "version": "0.1.0",
  "description": "A React Redux app set up with ParseServer",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf static/dist",
    "start": "npm run start-prod",
    "start-prod": "NODE_ENV=production node index.js",
    "start-dev": "node index.js",
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "heroku-postbuild": "npm run build",
    "watch-client": "node webpack-dev-server",
    "dev": "concurrent --kill-others \"npm run start-dev\" \"npm run watch-client\"",
    "lint": "eslint -c .eslintrc src/",
    "test": "NODE_ENV=test mocha ./src/**/__tests__/*.spec.js --compilers js:babel-core/register",
    "test:watch": "npm test -- --watch"
  },
  "author": "Zeb Pykosz <zeb.apy@gmail.com> (http://zeb.codes)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zebapy/react-redux-parse-server.git"
  },
  "bugs": {
    "url": "https://github.com/zebapy/react-redux-parse-server/issues"
  },
  "engines": {
    "node": "4.2.1",
    "npm": "3.3.9"
  },
  "dependencies": {
    "axios": "^0.11.0",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.5.0",
    "compression": "^1.6.1",
    "concurrently": "^2.0.0",
    "express": "^4.13.1",
    "normalizr": "^2.0.1",
    "parse-server": "^2.2.6",
    "radium": "^0.17.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.4",
    "react-router": "^2.1.1",
    "redux": "^3.4.0",
    "redux-form": "^5.0.1",
    "redux-thunk": "^2.0.1",
    "rimraf": "^2.5.0",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "webpack": "^1.12.15"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-plugin-react-transform": "^2.0.2",
    "eslint": "^3.3.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^5.0.1",
    "expect": "^1.16.0",
    "mocha": "^2.4.5",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.3",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.9",
    "redux-logger": "^2.5.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
